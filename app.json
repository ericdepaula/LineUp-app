    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#211e49"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.depaula39.contactexportapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.depaula39.contactexportapp",
