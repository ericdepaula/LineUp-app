    },
    
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf43c804-e2c0-4385-a94d-9ba5d3e083f9"
      }
    },
    "owner": "depaula39"
